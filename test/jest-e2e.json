{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@migrations/(.*)$": "<rootDir>/../migrations/$1",
    "^@test/(.*)$": "<rootDir>/$1",
    "^@commons/(.*)$": "<rootDir>/../src/commons/$1",
    "^@decorators/(.*)$": "<rootDir>/../src/commons/decorators/$1",
    "^@exceptions/(.*)$": "<rootDir>/../src/commons/exceptions/$1",
    "^@filters/(.*)$": "<rootDir>/../src/commons/filters/$1",
    "^@helpers/(.*)$": "<rootDir>/../src/commons/helpers/$1",
    "^@configs/(.*)$": "<rootDir>/../src/configs/$1",
    "^@endpoints/(.*)$": "<rootDir>/../src/endpoints/$1",
    "^@models/(.*)$": "<rootDir>/../src/models/$1",
    "^@schemas$": "<rootDir>/../src/schemas",
    "^@src/(.*)$": "<rootDir>/../src/$1"
  },
  "globalSetup": "./setup.ts"
}
